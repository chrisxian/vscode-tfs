{
  "name": "vscode-tfs",
  "description": "TFS Visual Studio Code integration.",
  "version": "0.6.0",
  "license": "MIT",
  "main": "./lib/vscode-tfs",
  "displayName": "TFS",
  "publisher": "ivangabriele",
  "icon": "res/icon.png",
  "engines": {
    "vscode": "^1.28.2"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-tfs.list",
        "title": "TFS: List Commands"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-tfs.get",
        "key": "Alt+G"
      },
      {
        "command": "vscode-tfs.status",
        "key": "Alt+S"
      },
      {
        "command": "vscode-tfs.list",
        "key": "Alt+T"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "tfs": "^1.4.2"
  },
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-githooks": "^0.6.0",
    "grunt-jsdoc": "^2.1.0",
    "grunt-mocha-test": "^0.13.2",
    "jshint-stylish": "^2.2.1",
    "mocha": "^3.2.0",
    "vscode": "^1.0.4"
  },
  "author": {
    "name": "Ivan Gabriele",
    "email": "ivan.gabriele@gmail.com",
    "url": "https://www.ivangabriele.com"
  },
  "galleryBanner": {
    "color": "#912a2a",
    "theme": "dark"
  },
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "keywords": [
    "tfs",
    "team",
    "foundation",
    "server",
    "microsoft"
  ], 
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivangabriele/vscode-tfs.git"
  },
  "bugs": {
    "url": "https://github.com/ivangabriele/vscode-tfs/issues"
  },
  "homepage": "https://github.com/ivangabriele/vscode-tfs#readme"
}
